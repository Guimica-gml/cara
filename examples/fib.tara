
func less(a: int, b: int): bool { __builtin_cmp_lt(a, b) }
func sub(a: int, b: int): int { __builtin_sub(a, b) }
func add(a: int, b: int): int { __builtin_add(a, b) }

func fib(n: int): int {
    if less(n, 2) { return n; };
    add(
      fib( sub(n, 1) ),
      fib( sub(n, 2) )
    )
}

func main(): int {
    let res = fib(6);
    res
}