
func _start() {
    // comments are like keywords
    // in that the `//` require to be presented on their own
    // do note that `/` otherwise is parsed as the separation token
    // for module paths, thus the following is invalid
    // ```
    //   let this//is//almost//a//name = 5;
    //   let this// = "does not a comment make";
    //   let //neither = this//;
    //   __builtin_syscall(60, this//is//a//name, 0, 0, 0, 0, 0);
    // ```
    __builtin_syscall(60, 0, 0, 0, 0, 0, 0);
}